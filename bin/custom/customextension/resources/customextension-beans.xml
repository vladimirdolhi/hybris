<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

    <bean class = "org.training.data.ContactRequestData">
        <property name = "message" type = "String" />
        <property name = "sender" type = "String" />
    </bean>

    <bean class="org.training.data.ItemWithAllAttributesData">
        <property name="name" type="String"/>
        <property name="enumProperty" type="org.training.enums.MyStaticEnum"/>
        <property name="mapProperty" type="java.util.Map&lt;String, String&gt;"/>
        <property name="listProperty" type="java.util.List&lt;Integer&gt;"/>
    </bean>

    <bean class="org.training.data.ConcertTourData">
        <property name="code" type="String"/>
        <property name="name" type="String"/>
        <property name="description" type="String"/>
    </bean>

    <bean class="org.training.data.ProducerData">
        <property name="code" type="String"/>
        <property name="firstname" type="String"/>
        <property name="lastname" type="String"/>
        <property name="concertTours" type="java.util.List&lt;org.training.data.ConcertTourData&gt;"/>
    </bean>

</beans>
